{
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "esModuleInterop": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "typeRoots": ["node_modules/@types", "cypress/support"]
  },
  "include": ["**/*.ts", "**/*.tsx", "cypress/**/*.ts"]
}