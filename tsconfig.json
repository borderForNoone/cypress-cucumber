{
  "compilerOptions": {
    "target": "ES6",
    "module": "commonjs",
    "esModuleInterop": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "typeRoots": ["node_modules/@types", "./types"],
    "types": ["cypress"]
  },
  "include": ["**/*.ts", "**/*.tsx", "cypress/**/*.ts"]
}